 . $def_ref
 . ./Default.gnome-terminal

# MAS_GTERMO_HAS_TABS="--tab -e '$MAS_DIR/gtbin/scs' --working-directory"
  MAS_GTERMO_HAS_TABS="--tab --working-directory"
  MAS_GTERMO_PROJECT=$MAS_DEVELOP_DIR/autotools/zoc-new/projects
